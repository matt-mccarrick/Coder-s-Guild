<%= form_for @tutorial do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :created_by %><br />
    <%= f.text_field :created_by %>
  </p>
  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :article %><br />
    <%= f.text_area :article %>
  </p>
  <p>
    <%= f.label :upvotes %><br />
    <%= f.text_field :upvotes %>
  </p>
  <p>
    <%= f.label :downvotes %><br />
    <%= f.text_field :downvotes %>
  </p>
  <p><%= f.submit %></p>
<% end %>
